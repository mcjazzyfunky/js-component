  "main": "./index.js",
  "module": "./dist/js-component.esm.production.js",
  "types": "dist/types/js-component.d.ts",
  "exports": {
    ".": {
      "development": {
        "browser": "./dist/js-component.umd.development.js",
        "umd": "./dist/js-component.umd.development.js",
        "import": "./dist/js-component.esm.development.js",
        "require": "./dist/js-component.cjs.development.js"
      },
      "production": {
        "browser": "./dist/js-component.umd.production.js",
        "umd": "./dist/js-component.umd.production.js",
        "import": "./dist/js-component.esm.production.js",
        "require": "./dist/js-component.cjs.production.js"
      },
      "default": {
        "browser": "./dist/js-component.umd.production.js",
        "umd": "./dist/js-component.umd.production.js",
        "import": "./dist/js-component.esm.production.js",
        "require": "./dist/js-component.cjs.production.js"
      }
    },
    "./hooks": {
      "development": {
        "browser": "./dist/js-component-hooks.umd.development.js",
        "umd": "./dist/js-component-hooks.umd.development.js",
        "import": "./dist/js-component-hooks.esm.development.js",
        "require": "./dist/js-component-hooks.cjs.development.js"
      },
      "production": {
        "browser": "./dist/js-component-hooks.umd.production.js",
        "umd": "./dist/js-component-hooks.umd.production.js",
        "import": "./dist/js-component-hooks.esm.production.js",
        "require": "./dist/js-component-hooks.cjs.production.js"
      },
      "default": {
        "browser": "./dist/js-component-hooks.umd.production.js",
        "umd": "./dist/js-component-hooks.umd.production.js",
        "import": "./dist/js-component-hooks.esm.production.js",
        "require": "./dist/js-component-hooks.cjs.production.js"
      }
    },
    "./utils": {
      "development": {
        "browser": "./dist/js-component-utils.umd.development.js",
        "umd": "./dist/js-component-utils.umd.development.js",
        "import": "./dist/js-component-utils.esm.development.js",
        "require": "./dist/js-component-utils.cjs.development.js"
      },
      "production": {
        "browser": "./dist/js-component-utils.umd.production.js",
        "umd": "./dist/js-component-utils.umd.production.js",
        "import": "./dist/js-component-utils.esm.production.js",
        "require": "./dist/js-component-utils.cjs.production.js"
      },
      "default": {
        "browser": "./dist/js-component-utils.umd.production.js",
        "umd": "./dist/js-component-utils.umd.production.js",
        "import": "./dist/js-component-utils.esm.production.js",
        "require": "./dist/js-component-utils.cjs.production.js"
      }
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "files": [
    "dist",
    "hooks",
    "utils"
  ],
  "scripts": {
    "clean": "shx rm -rf ./build ./dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "npm run clean && cross-env NODE_OPTIONS=--max_old_space_size=4096 rollup --c rollup.config.js && tsc -p tsconfig.dist.json -d --emitDeclarationOnly --declarationDir dist/types",
    "dist": "npm run build && npm run zipsource",
    "zipsource": "shx rm -rf ./build/source && shx mkdir -p ./build/source && copyfiles -a ./* ./build/source && shx cp -r ./src ./build/source && shx mkdir -p ./dist/source && cd build && bestzip --force node ../dist/source/source.zip source && cd .."
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
